CREATE DEFINER=`root`@`localhost` PROCEDURE `pGetEmployeeList`()
BEGIN
	SELECT
	    A.CEO_ID,
	    A.CEO_EMPLOYEE_NAME,
	    B.MANAGER_REPORTING_ID,
	    B.MANAGER_ID,
	    B.MANAGER_EMPLOYEE_NAME,
	    C.EMPLOYEE_ID,
	    C.EMPLOYEE_NAME,
	    C.EMPLOYEE_REPORTING_ID
	FROM
	    EMPLOYEECEO A 
		INNER JOIN EMPLOYEEMANAGER B ON A.CEO_ID = B.MANAGER_REPORTING_ID 
		INNER JOIN EMPLOYEE C ON B.MANAGER_ID = C.EMPLOYEE_REPORTING_ID
	ORDER BY A.CEO_ID, B.MANAGER_ID, C.EMPLOYEE_ID;
END